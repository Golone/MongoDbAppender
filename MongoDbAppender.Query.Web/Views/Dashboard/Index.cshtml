@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row" id="repos">
    @{ var repos = ViewBag.Repositories; }
    @for(var i = 0; i < repos.Count; i++) {
        var repo = ViewBag.Repositories[i] as MongoDbAppender.Query.Dto.LogRepositoryDto;
        <div class="col-md-3 repoContainer">
        </div>
    }
</div>

<script type="text/javascript">
    $("document").ready(function() {
        var repos = @(Html.Raw(ViewBag.RepositoryJson));
        loadRepos($("#repos"), repos);
    });
</script>
@Html.Partial("Templates/_Repository")